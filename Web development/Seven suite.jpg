    //API CALLER
    async function apiCaller(body, url, typeOfMsg) {
    
      body["message_type"] = typeOfMsg ? typeOfMsg : "text";
      body["service_type"] = "web";
      body["request_type"] = localStorage.getItem("currentService")
    
      let options = {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify(body)
      }
      let response = await fetch(
        `${url ? url : apiUrl}`,
        options);
      const data = await response.json();
    
      return data;
    }
    
    
    
    // To open/close chat area section
    function toggleChat() {
      isChatOpen ? isChatOpen = false : isChatOpen = true
      if (this.isChatOpen) {
        document.getElementById('chat-sec')?.classList.add('open');
        document.getElementById('float-icon-chat').classList.add('show-float-chat-icon');
      } else {
        document.getElementById('chat-sec')?.classList.remove('open');
        document.getElementById('float-icon-chat').classList.remove('show-float-chat-icon');
      }
    }
    
    
    //Function which will take data from user (button selection or input) and fetch data from API caller to update chat area , loading animation show and hide activity
    async function fetchChat(msgData, typeOfMsg) {
      console.log(msgData)
      let msgContent = document.querySelector('.messages-content');
      let child = document.createElement("div");
      child.classList.add("mCSB_container")
      child.innerHTML = '<div class="message loading "><span></span></div>';
      msgContent.append(child);
      let data = await apiCaller({
        sender: localStorage.getItem("mobileNumber"),
        message: msgData
      }, "", typeOfMsg)
      // setTimeout(() => {
        if (data) {
          document.querySelector('.loading').remove();
        }
      // }, 500)
      showMessages(data);
    }
    
    //function which will show chats depending on its type
    function showMessages(data) {
      data.forEach((element) => {
        let final = JSON.parse(element.text);
        final.forEach((innerEle) => {
          if (innerEle.type == "text") {
            enableInputArea();
            showTextMessage(innerEle);
          } else if (innerEle.type == "button") {
            disableInputArea();
            showBTN(innerEle)
          } else if (innerEle.type == "image") {
            disableInputArea();
            showImage(innerEle);
          } else if (innerEle.type == "input") {
            enableInputArea(innerEle);
            showTextMessage(innerEle);
          } else if (innerEle.type == "input_button") {
            enableInputArea();
            showBTN(innerEle)
          } else if (innerEle.type == "upload") {
            enableInputArea();
            showTextMessage(innerEle);
          }
        })
      });
    }
    
    
    function enableInputArea(input) {
      if (input?.phone) phoneInput = true;
      else phoneInput = false
      let inputSection = document.querySelector('.message-box');
      inputSection.className = inputSection.className.split(" ")[0];
    }
    function disableInputArea() {
      let inputSection = document.querySelector('.message-box');
      inputSection.className = inputSection.classList[0] + " input-action";
      let msgBox = document.querySelector('.message-input');
      msgBox.blur();
    }
    
    
    function showTextMessage(data) {
      let cls = (data.from == "user") ? "message-personal" : "";
      let botMessageCls = (data.from == "user") ? "notBotMessage" : "botMessage";  // here "notBotMessage" class is not used anywhere ... it is added cuz in classList.add() we can't add empty space classname ... that's y unusable class name is added
    
      let time = data.type == "input_button" ? "" : data.conversation_time ? new Date(data.conversation_time + "Z").toLocaleString(undefined, { timeZone: Intl.DateTimeFormat().resolvedOptions().timeZone }).split(",")[1].substring(0, 6) : new Date().getHours() + ":" + new Date().getMinutes();
    
      let msgContent = document.querySelector('.messages-content');
      let child = document.createElement("div");
      child.classList.add("mCSB_container", botMessageCls)
    
      child.innerHTML = `<div class="message ${cls} ">  ${data.data ? data.data : data.text}  <div class="timestamp">  ${time}  </div></div>`;
      msgContent.append(child);
    
      console.log(data.connect_to_live_agent)
      if(data.connect_to_live_agent){
    
        localStorage.setItem("currentService", "live_agent")
        setTimeout(() => {
          getConversationHistory(1, 10);
        }, 3000);
        handoverInterval = setInterval(() => {
          getConversationHistory(1, 2);
        }, 5000);
        scrollToTop();
      }
    
      scrollToTop();
    }
    
    function insertMessage() {
      let msg = document.querySelector('.message-input');
      if (msg.value.trim() == '') {
        return false;
      }
    
      if (phoneInput) {
    
        if (msg.value.trim().length != 10) return;
        let msgContent = document.querySelector('.messages-content');
        let child = document.createElement("div");
        child.classList.add("mCSB_container");
        child.innerHTML = `${'<div class="message message-personal">' + msg.value + '<div class="timestamp">' + new Date().getHours() + ":" + new Date().getMinutes() + '</div></div>'}`;
        msgContent.append(child);
        localStorage.setItem("currentService", "chatbot")
        localStorage.setItem("mobileNumber", msg.value);
        fetchChat("hi");
        phoneValue = ""
        msg.type = "text";
        msg.value = "";
        msg.placeholder = "Type message...";
        // for scoll top
        scrollToTop();
        return;
      } else {
        let msgContent = document.querySelector('.messages-content');
        let child = document.createElement("div");
        child.classList.add("mCSB_container");
        child.innerHTML = `${'<div class="message message-personal">' + msg.value + '<div class="timestamp">' + new Date().getHours() + ":" + new Date().getMinutes() + '</div></div>'}`;
        msgContent.append(child);
      }
    
      fetchChat(msg.value);
      msg.value = "";
      msg.placeholder = "Type message...";
      scrollToTop();
    }
    
    
    
    function showImage(data) {
      let cls = (data.from == "user") ? "uploaded-doc" : "";
      let msgContent = document.querySelector('.messages-content');
      let child = document.createElement("div");
      if (cls) {
        child.classList.add("mCSB_container", "img-section", "uploaded-doc");
      } else {
        child.classList.add("mCSB_container", "img-section");
      }
      // child.innerHTML = `${'<img src=' + data.data + ' class="actual-image" /><div class="img-caption">' + data.caption + '</div>'}`
      console.log(data)
      child.innerHTML = `<img src='${data.data}' class="actual-image ${data.type == "image-doc" ? "uploaded-doc-section" : ''}"/><div class="img-caption">${data?.caption ? data?.caption : ""}</div>`
      msgContent.append(child)
      setTimeout(() => {
        scrollToTop();
      }, 250)
    }
    
    
    function btnActionShow(text) {
      let msg = document.querySelector('.message-input');
      let msgContent = document.querySelector('.messages-content');
      let child = document.createElement("div");
      child.classList.add("mCSB_container");
      child.innerHTML = `${'<div class="message message-personal">' + text + '<div class="timestamp">' + new Date().getHours() + ":" + new Date().getMinutes() + '</div></div>'}`;
      msgContent.append(child);
    }
    
    
    function showBTN(data) {
      if (data.title) {
        let msgContentTitle = document.querySelector('.messages-content');
        let childTitle = document.createElement("div");
        childTitle.classList.add("mCSB_container", "botMessage");
        childTitle.innerHTML = `${'<div class="message ">' + data.title + '</div>'}`;
        msgContentTitle.append(childTitle)
      }
    
      if (data.live_agent) liveAgentHandOverButtonValue = data.live_agent;
    
      // for button option
      let listBrand = data.data;
      let msgContent = document.querySelector('.messages-content');
      let child = document.createElement("div");
      child.classList.add("mCSB_container", "btn-section")
      for (let i = 0; i < listBrand.length; i++) {
        let btn = document.createElement("button");
        btn.classList.add("btn-style")
        btn.onclick = function () {
          scrollToTop();
          btn.classList.remove("btn-style");
          btn.classList.add("selectedButton");
          btnActionShow(listBrand[i].button_text)
          buttonAction(listBrand[i].button_value);
          btn.classList.add("btn-disable");
          document.querySelectorAll('.btn-style').forEach(elem => {
            elem.classList.add('btn-disable')
            elem.disabled = true;
          });
    
        };
        btn.innerHTML = listBrand[i].button_text;
        child.appendChild(btn);
      }
    
      // this is to disable all buttons in case of fetch conversatison history
      setTimeout(() => {
        if (data.from == "bot") {
          document.querySelectorAll('.btn-style').forEach(elem => {
            elem.classList.add('btn-disable')
            elem.disabled = true;
          });
        }
      }, 100)
    
      msgContent.append(child);
      scrollToTop();
    }
    
    function buttonAction(val) {
      // console.log(liveAgentHandOverButtonValue);
      if (val == liveAgentHandOverButtonValue) { //Live agent handover activities
        fetchChat(val);
        localStorage.setItem("currentService", "live_agent")
        setTimeout(() => {
          getConversationHistory(1, 10);
        }, 3000);
        handoverInterval = setInterval(() => {
          getConversationHistory(1, 2);
        }, 5000);
      } else {  // normal button selection response
        fetchChat(val);
      }
      scrollToTop();
    }
    
    
    //helper fucntions 
    function scrollToTop() {
      let messageArea = document.querySelector('.messages');
      messageArea.scrollTop = messageArea.scrollHeight;
    }
    
    
    function ClearChat() {  // clean up activities ( local storage , chat array , conversation interval ,pagination variables , chat area )
      localStorage.clear();
      conversationChats = [];
      clearInterval(handoverInterval);
      pageNo = 0;
      isChatOpen ? isChatOpen = false : isChatOpen = true
      isChatConfirmOpen ? isChatConfirmOpen = false : isChatConfirmOpen = true;
      document.getElementById('chat-sec')?.classList.remove('chat-close-confirm');
      document.getElementById('float-icon-chat').classList.remove('show-float-chat-icon');
      document.getElementById('chat-sec')?.classList.remove('open');
      let msgContent = document.querySelector('.messages-content');
      msgContent.innerHTML = '';
      showInitialMessage();
    }
    
    function ClearChatConfirm() {
      isChatConfirmOpen ? isChatConfirmOpen = false : isChatConfirmOpen = true;
      if (this.isChatConfirmOpen) {
        document.getElementById('chat-sec')?.classList.add('chat-close-confirm');
        document.getElementById('float-icon-chat').classList.add('show-float-chat-icon');
      } else {
        document.getElementById('chat-sec')?.classList.remove('chat-close-confirm');
        document.getElementById('float-icon-chat').classList.remove('show-float-chat-icon');
      }
    }
    
    function showInitialMessage() {   // to show initial message 
      showMessages(initialMesssage);
      document.querySelector(".message-input").type = "Number";
    }
    
    async function getConversationHistory(pageNo, pageSize) {   // It'll get conversations frim api by passing required payload and show chats 
      enableInputArea();
      let payload = {
        "customer_identifier": localStorage.getItem("mobileNumber"),
        "service_type": "web",
        "page": pageNo || 1,
        "data_page_limit": pageSize || 5,
        "vendor": "BotBuilder",
        "message": "",
        "request_type": localStorage.getItem("currentService")
      }
    
      let data = await apiCaller(payload, apiUrl);
    
      if (data == "chatbot") {
        localStorage.setItem("currentService", "chatbot")
        clearInterval(handoverInterval);
        getConversationHistory(1);
        // return;
      } else {
        if (conversationChats.length >= 5 && data.page_number == 1) {
          if (conversationChats[conversationChats.length - 1]?.conversation_id != data.results[data.results.length - 1].conversation_id) {
            if (localStorage.getItem("currentService") == "chatbot") {
              data.results.forEach(ele => {
                conversationChats.push(ele)
              });
            } else {
    
    
              let filteredData = data.results.filter(ele => ele.conversation_id != conversationChats[conversationChats.length - 1]?.conversation_id)
              console.log(filteredData)
              filteredData.forEach(ele => {
                conversationChats.push(ele)
              })
              show();
              console.log("after msg send=>", conversationChats)
            }
          } else {
            return;
          }
        } else if (conversationChats.length >= 5 && data.page_number != 1) {
          data.results.forEach(ele => {
            conversationChats.unshift(ele)
          })
        } else {
          data?.results?.forEach(ele => {
            conversationChats.push(ele)
          })
        }
    
    
        if (conversationChats.length > 10) {
          if (data.page_number == 1) {
            if (localStorage.getItem('currentService') == "live_agent") {
