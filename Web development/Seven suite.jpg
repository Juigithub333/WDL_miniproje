    async function fileUpload() {
      let file = document.getElementById("fileUpload").files[0];
      console.log(file)
      if (file) {
        const data = new FormData();
        data.set('file', file);
        let res = await uploader(data);
        let imageFormat = `{\"type\": \"image-doc\",\"data\": \"${fileImage}\",\"caption\":\"<br>${res.data.fname}\",\"date\": \"\",\"from\":\"user\"}`;
        showImage(JSON.parse(imageFormat));
