    // return event.keyCode === 8 || event.charCode >= 48 && event.charCode <= 57
    
    
    
    function numCheck(event) {
      let ele = document.querySelector('.message-input');
      if (ele.type != "text") {
        if (event.keyCode == 69 || event.keyCode == 189) {
          ele.value = phoneValue.substring(0, phoneValue.length);
