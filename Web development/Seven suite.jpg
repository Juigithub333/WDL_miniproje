        let requestMsg = ''
        let type="";
        if(file.type.includes("image"))type="image";
        else if(file.type.includes("audio"))type="audio";
        else if(file.type.includes("video"))type="video";
        else type="document";
    
        if (res.data.success) {
          requestMsg = [{ type, data: res.data.Location }]
        } else {
          requestMsg = [{ type: "text", data: 'Upload Failed' }]
        }
        console.log("res", "'" + requestMsg + "'")
    
        fetchChat(requestMsg, type);
      }
    }
    
