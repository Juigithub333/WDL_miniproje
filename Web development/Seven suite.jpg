            enableInputArea();
            showImage(data);
          } else if (data.type=="input") { //innerEle.user_input.message[0].type == "input"
            enableInputArea();
            showTextMessage(data);
          } else if (data.type=="input_button") {
            enableInputArea();
            showBTN(data)
          } else if (data.type=="document") {
