                return;
              }
              else {
                show();
              }
            } else {
              show();
            }
          } else {
            show();
          }
        } else {
          show();
        }
      }
    }
    
    
    function show() {   // It'll show the chats depending on message type by itearting conversations chat array 
      let ele = document.querySelector('.messages-content');
      ele.innerHTML = '';
      let mainDivEle = document.getElementById("mainDiv");
    
      conversationChats.forEach((innerEle) => {
        if (innerEle.enduser_message) {
